{
    "presets": [
        ["@babel/preset-env", {
            "targets": [
                "last 2 versions",
                "not dead",
                "not < 2%",
                "ie 11"
            ],
            "useBuiltIns": "usage",
            "corejs": {
                "version": 3,
                "proposals": true
            }
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import"
    ]
}